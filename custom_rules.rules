<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Custom Build Rules"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="UI"
			DisplayName="Compile QT UserInterface File (*.ui)"
			CommandLine="$(QTDIR)/bin/uic.exe &quot;$(InputPath)&quot; -o &quot;$(InputDir)\ui_$(InputName).h&quot;"
			Outputs="$(InputDir)ui_$(InputName).h"
			FileExtensions="*.ui"
			ExecutionDescription="Creating UI File ui_$(InputName).h"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="QT resource Compile"
			DisplayName="QT qrc compile"
			CommandLine="$(QTDIR)/bin/rcc.exe -name $(InputName) &quot;$(InputPath)&quot; -o &quot;$(InputDir)/qrc_$(InputName).cpp&quot;"
			Outputs="$(InputDir)qrc_$(InputName).cpp"
			FileExtensions="*.qrc"
			ExecutionDescription="Compiling QT resource file "
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
		<CustomBuildRule
			Name="QT MOC"
			DisplayName="QT MOC"
			CommandLine="$(QTDIR)\bin\moc.exe &quot;$(InputPath)&quot; -o &quot;$(ProjectDir)\moc_$(InputName).cpp&quot;"
			Outputs="$(ProjectDir)\moc_$(InputName).cpp"
			FileExtensions="*.h"
			ExecutionDescription="Performing moc on $(InputName).h"
			ShowOnlyRuleProperties="false"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
